{
  "buildCommand": "ng build --configuration production",
  "outputDirectory": "dist/repairhubcoreui/browser",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": null,
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\.js)",
      "dest": "/$1"
    },
    {
      "src": "/(.*\\.css)",
      "dest": "/$1"
    },
    {
      "src": "/(.*\\.woff2)",
      "dest": "/$1"
    },
    {
      "src": "/(.*\\.woff)",
      "dest": "/$1"
    },
    {
      "src": "/(.*\\.ttf)",
      "dest": "/$1"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}
